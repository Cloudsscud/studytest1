#include<stdio.h>

//在屏幕上打印一个9*9乘法口诀表
int main()
{
	int i, j;
	for (i = 1; i <= 9; i++) //行数
	{
		for (j = 1; j <= i; j++) //列数
		{
			printf("%d*%d=%2d ", i, j, i * j);
		}
		printf("\n");
	}
	return 0;
}

